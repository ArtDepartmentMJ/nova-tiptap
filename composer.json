{
  "name": "marshmallow/nova-tiptap",
  "description": "A Laravel Nova tiptap editor field.",
  "keywords": [
    "laravel",
    "nova",
    "tiptap",
    "vue",
    "editor"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/nova": "^4.0|^5.0"
  },
  "autoload": {
    "psr-4": {
      "Marshmallow\\Tiptap\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Marshmallow\\Tiptap\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
